<div class="graph">
    <div class="comBox-container">
        <select name="measures" class="select-measures">
            <option value="0">Temperatura</option>
            <option value="1">RH</option>
            <option value="2">Presión atm</option>
            <option value="3">PM10_1</option>
            <option value="4">PM10_2</option>
            <option value="5">PM25_1</option>
            <option value="6">PM25_2</option>
        </select>
    </div>
    <canvas id="myChartModulair"></canvas>
    <p class="p-dates"></p>
</div>
<section class="sensorData">
    <h2 class="titulo">Datos Realtime</h2>
    <h3 id="subtitle"></h3>
    <div class="container-table">
        <table class="table" style="background-color: #014691;">
            <thead class="theadModul">
                <tr>
                    <th scope="col">Medida</th>
                    <th scope="col">Valores</th>
                </tr>
            </thead>
            <tbody class=" table" style="color: #fff;">
                <tr class="table-active">
                    <th scope="row">Temperatura</th>
                    <td id="temp">

                    </td>
                </tr>
                <tr class="table-active">
                    <th scope="row">rh</th>
                    <td id="rh">

                    </td>
                </tr>
                <tr class="table-active">
                    <th scope="row">Presión atm</th>
                    <td id="atmPressure">

                    </td>
                </tr>
                <tr class="table-active">
                    <th scope="row">PM10_1 Conc. masas</th>
                    <td id="pm10_1">

                    </td>
                </tr>
                <tr class="table-active">
                    <th scope="row">PM10_2 Conc. masas</th>
                    <td id="pm10_2">

                    </td>
                </tr>
                <tr class="table-active">
                    <th scope="row">PM25_1 Conc. masas</th>
                    <td id="pm25_1">

                    </td>
                </tr>
                <tr class="table-active">
                    <th scope="row">PM25_2 Conc. masas</th>
                    <td id="pm25_2">

                    </td>
                </tr>
                <tr class="table-active">
                    <th scope="row">Fecha de muestreo</th>
                    <td id="date">

                    </td>
                </tr>
                <tr class="table-active">
                    <th scope="row">Hora de muestreo</th>
                    <td id="hour">

                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</section>
<section class="sectionInfo">
    <h2 class="titulo">Información del sensor</h2>
    <div class="container-table">
        <table class="table">
            <thead class="theadModul">
                <tr>
                    <th scope="col">Type</th>
                    <th scope="col">Información</th>
                </tr>
            </thead>
            <tbody class=" table" style="color: #fff;">
                <tr class="table-active">
                    <th scope="row">Nombre dispositivo</th>
                    <td id="deviceName">

                    </td>
                </tr>
                <tr class="table-active">
                    <th scope="row">Dispositivo</th>
                    <td id="device">

                    </td>
                </tr>
                <tr class="table-active">
                    <th scope="row">Estado</th>
                    <td id="status">
                    </td>
                </tr>
                <tr class="table-active">
                    <th scope="row">País</th>
                    <td id="country">
                    </td>
                </tr>
                <tr class="table-active">
                    <th scope="row">Tipo</th>
                    <td id="type">

                    </td>
                </tr>
                <tr class="table-active">
                    <th scope="row">Latitud</th>
                    <td id="lat">

                    </td>
                </tr>
                <tr class="table-active">
                    <th scope="row">Longitud</th>
                    <td id="lon">

                    </td>
                </tr>
                <tr class="table-active">
                    <th scope="row">Batería</th>
                    <td>
                        NA
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</section>

<script type="module" src="/js/eva.js"></script>